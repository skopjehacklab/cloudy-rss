{
	"name": "@cloudy-rss/frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "sst bind vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
